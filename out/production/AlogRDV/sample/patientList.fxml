<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.PatientListController">
    <children>
        <TableView fx:id="tablePatient" prefHeight="400.0" prefWidth="600.0">
            <columns>
                <TableColumn fx:id="id" prefWidth="38.0" text="id"/>
                <TableColumn fx:id="nom" prefWidth="67.0" text="Nom"/>
                <TableColumn fx:id="prenom" prefWidth="67.0" text="Prénom"/>
                <TableColumn fx:id="adresse" prefWidth="86.0" text="Adresse"/>
                <TableColumn fx:id="tel" prefWidth="102.0" text="Téléphone"/>
                <TableColumn fx:id="mail" prefWidth="111.0" text="Email"/>
                <TableColumn fx:id="infosMed" prefWidth="128.0" text="Informations"/>
            </columns>
        </TableView>
    </children>
</AnchorPane>